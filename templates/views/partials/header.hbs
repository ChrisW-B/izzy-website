<div class="header">
  <a class="name" href='/'><img src='/images/name.png' /></a>
  <h1 class="page-title">{{title}}</h1>
  <div class="mini-menu">
    <button class="menu-hex" id="headerButton"></button>
    <ul class="menu-items" id="headerMenu">
      {{# each navLinks}}
        {{#ifeq ../section key}}
          {{else}}
            <li class="menu-item {{key}}">
              <a href="{{ href }}">{{ label }}</a>
            </li>
        {{/ifeq}}
        {{/each}}
    </ul>
  </div>
</div>
<link href="/styles/header.css" rel="stylesheet" />
<script src="/js/header.js"></script>
